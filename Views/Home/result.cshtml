<!DOCTYPE html>
<html>
    <head>
        <meta charset='utf-8'>
        <title>Results</title>
        
        <link rel="stylesheet" href="~/css/style.css"/>
    </head>
    <body>
      

        <h3> Here are some Awesome Quotes!!!! </h3>
        <h4> Name : @ViewBag.Name </h4>
        <h4> Quote: @ViewBag.Quote </h4>
         <a href ="/"><button> Go Back </button></a>
            <h3> List of Quotes! </h3>
            <table>
            <tr>
                <th> Name </th>
                <th> Quote </th>
            </tr>
            @foreach(var q in @ViewBag.allQuotes){
                <tr>
                    <th> @q["Name"] </th>
                    <th> @q["Quote"] </th>
                </tr>
            }
        </table> 
    </body>
</html>